{
  "name": "practice_2-tests-with-jest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "parcel index.html",
    "test": "jest",
    "verify": "npm run format:check && npm run test:coverage",
    "format": "prettier --write *.{js,jsx} __tests__/**/*.{js,jsx}",
    "format:check": "prettier --check *.{js,jsx} __tests__/**/*.{js,jsx}",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-jest": "^27.4.6",
    "jest": "^27.4.7",
    "parcel": "^2.2.1",
    "prettier": "^2.5.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "*.jsx"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  }
}
